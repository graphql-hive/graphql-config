{
  "name": "@graphql-config/website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint --ignore-path ../.gitignore --ext js,jsx,cjs,mjs,ts,tsx,cts,mts .",
    "dev": "next",
    "build": "next build",
    "pagefind": "pagefind --site .next/server/app --output-path out/_pagefind",
    "postbuild": "next-sitemap && pnpm pagefind",
    "analyze": "cross-env ANALYZE=true pnpm run build"
  },
  "dependencies": {
    "@theguild/components": "9.8.0",
    "next": "^15.3.3",
    "next-sitemap": "^4.2.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@theguild/tailwind-config": "0.6.3",
    "@types/node": "22.19.13",
    "@types/react": "19.1.8",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "cross-env": "7.0.3",
    "eslint": "9.24.0",
    "eslint-config-next": "15.5.7",
    "eslint-config-prettier": "10.1.8",
    "pagefind": "1.3.0",
    "postcss-import": "16.1.1",
    "postcss-lightningcss": "1.0.1",
    "tailwindcss": "3.4.17",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">= 16.0.0",
    "pnpm": "^10.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
